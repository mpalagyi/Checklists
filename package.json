{
  "name": "checklists",
  "description": "Checklists written in Markdown and then converted to PDFs",
  "author": "Rob Prouse <rob@prouse.org>",
  "license": "MIT",
  "packageManager": "yarn@3.0.2",
  "scripts": {
    "clean": "del-cli -f pdfs",
    "build": "md-to-pdf --config-file .config.json ./**/*.md && mkdirp pdfs && cpy '**/*.pdf' 'pdfs/*.pdf' pdfs"
  },
  "devDependencies": {
    "cpy-cli": "^3.1.1",
    "md-to-pdf": "^5.0.0",
    "mkdirp": "^1.0.4",
    "move-file-cli": "^3.0.0"
  }
}
